        "gatsby-transformer-sharp": "^4.24.0",
        "graphql": "^15.0.0",
        "prismjs": "^1.28.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-helmet": "^6.1.0",
