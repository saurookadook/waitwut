{
    "name": "waitwut",
    "version": "1.0.0",
    "description": "A little Gatsby-fueled blog with a bunch of language syntax cheatsheets ðŸ™ƒ",
    "author": "Andy Maskiell",
    "keywords": [
        "gatsby"
    ],
    "engines": {
        "node": "^18.20.0",
        "yarn": "^4.1.1"
    },
    "packageManager": "yarn@4.1.1",
    "scripts": {
        "safe-rm-ycache": "rm -rf .yarn/ || true",
        "safe-rm-node-modules": "rm -rf node_modules/ || true",
        "safe-rm-public": "rm -rf public/ || true",
        "clean": "yarn safe-rm-ycache && yarn safe-rm-public && gatsby clean",
        "clean:cache": "yarn clean && yarn cache clean",
        "install:clean": "yarn clean:cache && yarn safe-rm-node-modules && yarn install && yarn build",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "deploy": "gatsby clean && gatsby build --prefix-paths && gh-pages -d public",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ./ --fix",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mdx-js/mdx": "^1.0.0",
        "@mdx-js/react": "^1.0.0",
        "@mui/icons-material": "^5.14.3",
        "@mui/lab": "^5.0.0-alpha.75",
        "@mui/material": "^5.5.3",
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^29.5.3",
        "@types/node": "^17.0.45",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/react-helmet": "^6.1.6",
        "@types/styled-components": "^5.1.24",
        "babel-plugin-styled-components": "^1.5.0",
        "classnames": "^2.3.2",
        "gatsby": "^4.18.2",
        "gatsby-plugin-image": "^2.24.0",
        "gatsby-plugin-mdx": "^3.19.0",
        "gatsby-plugin-netlify": "^5.1.1",
        "gatsby-plugin-sharp": "^4.24.0",
        "gatsby-plugin-styled-components": "^5.18.0",
        "gatsby-remark-autolink-headers": "^5.19.0",
        "gatsby-remark-images": "^6.18.0",
        "gatsby-remark-prismjs": "^6.19.0",
        "gatsby-source-filesystem": "^4.19.0",
